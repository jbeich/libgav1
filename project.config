[access]
	inheritFrom = All-Projects
[submit]
	action = inherit
[access "refs/*"]
	owner = group codec-admins
	owner = group mdb/chrome-git-admins
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[access "refs/heads/*"]
	push = group codec-admins
[access "refs/for/*"]
	label-Code-Review = -2..+2 group codec-admins
